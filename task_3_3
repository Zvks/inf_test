// Массив имеет размер 10 элементов. Заполнить массив числыми, введенными с клавиатуры. Элементы массива, кратные 3, обнулить. Вывести на экран обновленный массив.

#include <iostream>
using namespace std;
int main() {
    int A_arr[10];
    for (int i = 0; i < 10; i++) {
        cin >> A_arr[i];
    }
    for (int i = 0; i < 10; i++) {
        if (A_arr[i] % 3 == 0) {A_arr[i] = 0;}
    }
    for (int i = 0; i < 10; i++) {
        cout << A_arr[i] << ' ';
    }
    return 0;
}

